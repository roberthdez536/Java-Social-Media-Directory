<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GUIController">
   <children>
      <TabPane prefHeight="600.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="view users">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="UsersTable" layoutX="131.0" layoutY="30.0" prefHeight="266.0" prefWidth="644.0">
                          <columns>
                            <TableColumn fx:id="colId" prefWidth="60.0" text="id" />
                            <TableColumn fx:id="colUname" minWidth="0.0" prefWidth="142.4000244140625" text="uname" />
                              <TableColumn fx:id="colPass" minWidth="4.0" prefWidth="286.39996337890625" text="pass" />
                              <TableColumn fx:id="colEmail" minWidth="0.0" prefWidth="144.0" text="email" />
                          </columns>
                        </TableView>
                        <Button fx:id="myButton" layoutX="253.0" layoutY="418.0" mnemonicParsing="false" onAction="#saySomething" text="Button" />
                        <Button fx:id="refreshButton" layoutX="61.0" layoutY="30.0" mnemonicParsing="false" onAction="#refreshTable" text="Refresh" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="add users">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
           <Tab text="remove users">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
           <Tab text="edit users">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
